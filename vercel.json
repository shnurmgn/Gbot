{
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/bot.py"
    }
  ],
  "crons": [
    {
      "path": "/api/process-queue?cron_secret=MySuperSecretCronKey123",
      "schedule": "* * * * *"
    }
  ]
}
